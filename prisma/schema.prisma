generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Apoio {
  id        Int    @id @default(autoincrement())
  nome      String
  contato   String
  url       String
  descricao String
}

model Autocuidado {
  id        Int    @id @default(autoincrement())
  titulo    String
  descricao String
}

model Bullying {
  id         Int    @id @default(autoincrement())
  tipo       String
  descricao  String
  como_agir  String
}